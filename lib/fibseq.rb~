class FibSeq

	def initialize(num)
		@num = num
		return generate(@num).to_s
	end

	def generate ( iterations )
		i = 0
		j = 0
		oldnumber = 0
		nextnumber = 1
		@fibarray = Array.new
		iterations.times { @fibarray<< "Iteration number #{j} number is #{nextnumber}\n"
    oldnumber = i 
	  i = nextnumber
		nextnumber += oldnumber   
	  j += 1 }
	@fibarray
end

end


